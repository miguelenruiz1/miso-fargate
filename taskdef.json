{
    "executionRoleArn": "arn:aws:iam::988732899429:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "portMappings": [
          {
            "hostPort": 5000,
            "protocol": "tcp",
            "containerPort": 5000
          }
        ],

        "image": "<IMAGE1_NAME>",
        "essential": true,
        "name": "Container-app-python",
        "environment": [
        {
          "name": "AUTH_TOKEN",
          "value": "my_static_token"
        },
        {
          "name": "DATABASE_HOST",
          "value": "blacklist-db.c6tgkk6a4m81.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DATABASE_PASSWORD",
          "value": "postgres1234"
        },
        {
          "name": "RDS_PORT",
          "value": "5432"
        },
        {
          "name": "DATABASE_USER",
          "value": "postgres"
        },
        {
          "name": "DATABASE_NAME",
          "value": "blacklist_db"
        }
      ],
       "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/Task-app-python",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
        
      }
    ],
    

    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "Task-app-python"
  }