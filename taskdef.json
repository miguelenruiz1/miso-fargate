{
  "family": "Task-app-python",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::988732899429:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "Container-app-python-miso",
      "image": "IMAGE1_NAME",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
